  "private": true,
  "scripts": {
    "start": "pm2 startOrReload ecosystem.config.js --env production",
    "start:local": "cross-env NODE_ENV=production PORT=3000 pm2 start server.js -i -1",
    "stop:local": "pm2 kill",
    "start:ngrok": "cross-env NODE_ENV=production NGROK=true PORT=3000 pm2 start server.js -i -1",
    "dev": "cross-env NODE_ENV=development node server.js",
    "stop": "pm2 stop ecosystem.config.js --env production",
    "restart": "pm2 restart ecosystem.config.js --env production",
    "build": "cross-env NODE_ENV=production webpack",
    "deploy": "pm2 deploy ecosystem.config.js production",
    "deploy:setup": "pm2 deploy ecosystem.config.js production setup",
    "ncu": "ncu",
    "ncu:update": "ncu --upgrade",
    "test": "PORT=2999 mocha ./www --recursive",
    "cover": "PORT=2999 nyc --reporter=html --reporter=lcov --reporter=text --reporter=text-summary --require babel-register --require babel-polyfill --report-dir coverage mocha ./handlers --recursive",
    "postmerge": "yarn test",
    "precommit": "yarn test",
    "prepush": "yarn test",
    "tdd": "",
    "redis:development": "redis-server ./config/redis.development.conf",
    "redis:production": "redis-server ./config/redis.conf",
    "mongodb:development": "sudo mongod --config ./config/mongod.development.conf",
    "mongodb:production": "sudo mongod --config ./config/mongod.conf"
  },
